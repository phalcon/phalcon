{
    "name": "phalcon/phalcon",
    "description": "Phalcon Framework",
    "keywords": [
        "php",
        "framework",
        "psr-3",
        "psr-4",
        "psr-7",
        "psr-11",
        "psr-13",
        "psr-14",
        "psr-15",
        "psr-16",
        "psr-17",
        "psr-18"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4 <8.0",
        "ext-apcu": "*",
        "ext-igbinary": "*",
        "ext-json": "*",
        "ext-mbstring": "7.*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-xml": "^7.2",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/link": "^1.0",
        "psr/log": "^1.1",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Phalcon\\": "src/",
            "Phalcon\\Tests\\Benchmarks\\": "tests/benchmarks/",
            "Phalcon\\Tests\\Fixtures\\": "tests/_data/fixtures/",
            "Phalcon\\Tests\\Unit\\": "tests/unit/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require-dev": {
        "codeception/codeception": "^4.0",
        "codeception/module-apc": "^1.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-db": "^1.0",
        "codeception/module-filesystem": "^1.0.0",
        "codeception/module-redis": "^1.0",
        "pds/skeleton": "^1.0",
        "phpbench/phpbench": "^0.17.1",
        "phpstan/phpstan": "^0.11.19",
        "squizlabs/php_codesniffer": "^3.4",
        "vlucas/phpdotenv": "^4.0"
    }
}
